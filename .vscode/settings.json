{
	"editor.insertSpaces": false,
	"[fsharp]": { "editor.insertSpaces": true },
	"eslint.validate": ["typescript"],
	"files.exclude": {
		"**/bin": true,
		"**/obj": true,
		"**/*.css": true,
		"**/*.css.map": true,
		"**/*.js": { "when": "$(basename).ts" },
		"**/*.js.map": true,
		"**/*.fs": true,
		".ionide": true,
		"dist": true,
		"LICENSE": true,
		"node_modules": true,
		"package-lock.json": true
	},
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true
}
